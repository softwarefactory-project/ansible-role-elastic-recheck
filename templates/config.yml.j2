---
_meta:
  config_version: 2
  type: config
config:
  dynamic:
    kibana:
      multitenancy_enabled: true
    http:
      anonymous_auth_enabled: false
      xff:
        enabled: false
    authc:
      basic_internal_auth_domain:
        http_enabled: true
        transport_enabled: true
        order: 1
        authentication_backend:
          type: intern
        http_authenticator:
          type: basic
          challenge: true
